  <!--  Number counter  -->
  <script>
(function() {
  const animateNumber = (element, target, duration) => {
    let startTime;
    const initialNumber = 0;
    const easingFunction = t => 1 - Math.pow(1 - t, 4);
    const animate = time => {
      if (!startTime) startTime = time;
      const elapsedTime = time - startTime;
      const t = Math.min(elapsedTime / duration, 1);
      const newValue = initialNumber + (target - initialNumber) * easingFunction(t);
      element.textContent = Math.round(newValue);
      if (elapsedTime < duration) {
        requestAnimationFrame(animate);
      } else {
        element.textContent = target;
      }
    };
    requestAnimationFrame(animate);
  };
  const onIntersection = entries => {
    entries.forEach(entry => {
      if (entry.isIntersecting) {
        const el = entry.target;
        const finalNumber = parseInt(el.textContent, 10);
        const animDuration = parseInt(el.getAttribute('duration'), 10) || 2000;
        animateNumber(el, finalNumber, animDuration);
        observer.unobserve(el);
      }
    });
  };
  const observer = new IntersectionObserver(onIntersection);
  document.addEventListener("DOMContentLoaded", () => {
    document.querySelectorAll('[counter-element="number"]').forEach(el => {
      observer.observe(el);
    });
  });
})();
</script>
  <script>
  document.addEventListener('DOMContentLoaded', () => {
    const logoNavDesktop = document.querySelectorAll('.logo-nav-desktop');
    const logoNavMobile = document.querySelectorAll('.logo-nav-mobile');
    const navToggles = document.querySelectorAll('.navigation-toggle');
    const navCovers = document.querySelectorAll('.nav-cover');
    const menuIconDarks = document.querySelectorAll('.menu-icon-dark');
    const menuIconWhites = document.querySelectorAll('.menu-icon-white');
    const currencyToggles = document.querySelectorAll('.currency-toggle');
    const menuButtons = document.querySelectorAll('.menu-button');
    const menuTriggers = document.querySelectorAll('.menu-trigger');
    const mobileMenus = document.querySelectorAll('.menu-mob-container');
    const navigationWrap = document.querySelector('.navigation-wrap');
    const isDarkVariant = document.querySelector('[data-wf--navigation--variant="dark"]') !== null;
    let isMenuOpen = false;
    logoNavDesktop.forEach(el => el.style.transition = 'filter 0.3s ease');
    logoNavMobile.forEach(el => el.style.transition = 'filter 0.3s ease');
    navToggles.forEach(el => el.style.transition = 'color 0.3s ease');
    navCovers.forEach(el => el.style.transition = 'transform 0.3s ease');
    menuIconDarks.forEach(el => el.style.transition = 'opacity 0.3s ease');
    menuIconWhites.forEach(el => el.style.transition = 'opacity 0.3s ease');
    currencyToggles.forEach(el => el.style.transition = 'color 0.3s ease');
    menuButtons.forEach(el => {
      el.style.transition = 'color 0.3s ease, border-color 0.3s ease';
    });
    menuTriggers.forEach(el => el.style.transition = 'filter 0.3s ease');
    mobileMenus.forEach(el => el.style.transition = 'opacity 0.3s ease');
    navigationWrap.style.transition = 'background-color 0.3s ease';
    function updateLogoBasedOnScroll() {
      const scrollPercent = (window.scrollY / window.innerHeight) * 100;
      logoNavDesktop.forEach(el => {
        if (isMenuOpen) return;
        el.style.filter = isDarkVariant ? 'none' : (scrollPercent >= 2 ? 'none' : 'invert(1)');
      });
      logoNavMobile.forEach(el => {
        if (isMenuOpen) return;
        el.style.filter = isDarkVariant ? 'none' : (scrollPercent >= 2 ? 'none' : 'invert(1)');
      });
    }
    menuTriggers.forEach(trigger => {
      trigger.addEventListener('click', () => {
        const isHidden = mobileMenus[0].style.display === 'none' || mobileMenus[0].style.display === '';
        mobileMenus.forEach(menu => {
          menu.style.display = isHidden ? 'flex' : 'none';
        });
        if (isHidden) {
          navigationWrap.style.transition = 'background-color 0.3s ease';
          navigationWrap.style.backgroundColor = '#0c0c0c';
          menuTriggers.forEach(el => el.style.filter = isDarkVariant ? 'none' : 'invert(1)');
          logoNavDesktop.forEach(el => el.style.filter = 'invert(1)');
          logoNavMobile.forEach(el => el.style.filter = 'invert(1)');
        } else {
          navigationWrap.style.transition = 'none';
          navigationWrap.style.backgroundColor = 'transparent';
          updateLogoBasedOnScroll();
          const scrollPercent = (window.scrollY / window.innerHeight) * 100;
          menuTriggers.forEach(el => el.style.filter = scrollPercent >= 2 ? 'none' : 'invert(1)');
        }
        isMenuOpen = !isHidden;
      });
    });
    window.addEventListener('scroll', () => {
      // Skip ALL scroll changes when menu is open
      if (mobileMenus[0].style.display === 'flex') return;
      // Skip scroll effects if dark variant
      if (isDarkVariant) return;
      const scrollPercent = (window.scrollY / window.innerHeight) * 100;
      if (scrollPercent >= 2) {
        navToggles.forEach(el => el.style.color = '#53565a');
        navCovers.forEach(el => el.style.transform = 'translateY(0%)');
        menuIconDarks.forEach(el => el.style.opacity = 1);
        menuIconWhites.forEach(el => el.style.opacity = 0);
        currencyToggles.forEach(el => el.style.color = '#53565a');
        menuButtons.forEach(el => {
          el.style.color = '#53565a';
          el.style.borderColor = '#53565a';
        });
        menuTriggers.forEach(el => el.style.filter = 'none');
        logoNavDesktop.forEach(el => el.style.filter = 'none');
        logoNavMobile.forEach(el => el.style.filter = 'none');
      } else {
        navToggles.forEach(el => el.style.color = 'white');
        navCovers.forEach(el => el.style.transform = 'translateY(-100%)');
        menuIconDarks.forEach(el => el.style.opacity = 0);
        menuIconWhites.forEach(el => el.style.opacity = 1);
        currencyToggles.forEach(el => el.style.color = 'white');
        menuButtons.forEach(el => {
          el.style.color = 'white';
          el.style.borderColor = 'white';
        });
        menuTriggers.forEach(el => el.style.filter = 'invert(1)');
        logoNavDesktop.forEach(el => el.style.filter = 'invert(1)');
        logoNavMobile.forEach(el => el.style.filter = 'invert(1)');
      }
    });
    // Initial state
    menuTriggers.forEach(el => {
      el.style.filter = isDarkVariant ? 'none' : 'invert(1)';
    });
    updateLogoBasedOnScroll();
  });
</script>
