<div style="background-color: {{ section.settings.background_colour }} !important;">
          <div class="padding-global-6 padding-section-small">
            <div class="container-large">
              <div class="content_grid">
                <div li-content-for-blocks="" class="content_wrapper-2">
{% for block in section.blocks %}
{% case block.type %}
{% when '@app' %}
{% render block %}

                  {% when 'headline' %}
            <div li-block="Headline" class="content_block">
                    
                    
                    <h2>
{{ block.settings.text_headline }}
</h2>
                    
                  
                </div>
                  {% when 'headline-small' %}
            <div li-block="Headline small" class="content_block">
                    
                    
                    <div class="eyebrow">
{{ block.settings.text_text }}
</div>
                    
                  
                </div>
                  {% when 'text' %}
            <div li-block="Text" class="content_block">
                    
                    
                    <div> {{ block.settings.richtext_text }} </div>
                    
                  
                </div>
                  {% when 'checklist' %}
            <div li-block="Checklist" class="grid_2-col">
                    
                    
                    <div class="icon-text_row-small">
                      <div class="keep-sizing"><img alt="" src="{{ 'check_circle_gray20-primary.svg' | asset_url }}" class="icon-xxsmall"></div>
                      <p class="text-size-small-23">
{{ block.settings.text_checklist_text }}
</p>
                    </div>
                    
                    
                    <div class="icon-text_row-small">
                      <div class="keep-sizing"><img alt="" src="{{ 'check_circle_gray20-primary.svg' | asset_url }}" class="icon-xxsmall"></div>
                      <p class="text-size-small-23">
{{ block.settings.text_checklist_text }}
</p>
                    </div>
                    
                  
                </div>
                  {% when 'buttons' %}
            <div li-block="Buttons" class="content_block">
                    
                    
                    <div class="button-group-10">
                      {% if block.settings.url_button_link %}    <div li-if="block.settings.url_button_link" class="content_block">
    
                        <a href="{{ block.settings.url_button_link }}" data-wf--button--variant="base" data-w-id="47ee13e6-1a71-57c3-88e5-9d80ec55d256" class="button w-inline-block"> <div class="btn-txt-container"> <div class="btn-txt">{{ block.settings.text_button_text }}</div> <div class="btn-txt">{{ block.settings.text_button_text }}</div> </div> </a>
                          </div>
{% endif %}
                      {% if block.settings.url_button_link_2 %}    <div li-if="block.settings.url_button_link_2" class="content_block">
    
                        <a href="{{ block.settings.url_button_link_2 }}" data-wf--button--variant="secondary" data-w-id="47ee13e6-1a71-57c3-88e5-9d80ec55d256" class="button w-variant-554ce3f9-7bdf-5ed8-135f-467a5c40a409 w-inline-block"> <div class="btn-txt-container"> <div class="btn-txt">{{ block.settings.text_button_text }}</div> <div class="btn-txt">{{ block.settings.text_button_text }}</div> </div> </a>
                          </div>
{% endif %}
                    </div>
                    
                  
                </div>
                {% endcase %}
{% endfor %}

    </div>
                <div id="w-node-_1c2acbc7-448b-5b0c-8350-cbc453daa35e-53daa35a" class="content_image-wrapper" style="{% if section.settings.grid_position == 'left' %}order: -1;{% endif %}">{% if section.settings.image_image %}
{{ section.settings.image_image | image_url: width: 1000 | image_tag: loading: 'lazy', class: 'content_image' }}
{% else %}
<img alt="" src="{{ 'image.svg' | asset_url }}" loading="lazy" class="content_image">
{% endif %}</div>
              </div>
            </div>
          </div>
