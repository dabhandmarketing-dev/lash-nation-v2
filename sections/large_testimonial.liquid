<div class="padding-global">
            <div class="container-large">
              <div data-w-id="adb45241-c7ed-67e9-487b-a7ac699fe60c" class="testimonial-card">
                <div class="testimonial-image-small">{% if section.settings.image_image %}
{{ section.settings.image_image | image_url: width: 1000 | image_tag: class: 'image-cover', sizes: '(max-width: 1080px) 100vw, 1080px', loading: 'lazy' }}
{% else %}
<img class="image-cover" src="{{ 'Review-Photo.jpg' | asset_url }}" alt="" sizes="(max-width: 1080px) 100vw, 1080px" loading="lazy" srcset="{{ 'Review-Photo-p-500.jpg' | asset_url }} 500w, {{ 'Review-Photo-p-800.jpg' | asset_url }} 800w, {{ 'Review-Photo.jpg' | asset_url }} 1080w">
